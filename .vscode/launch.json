  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "gcc build an active file",
      "name": "Debugging toNum",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/assemble",
      "args": ["more_test_cases/fail0_bad_label.txt", "output.obj"],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}